    </fragment>

    <fragment
        android:id="@+id/cartFragment"
        android:name="com.jamshidbek.marketplaceinc.mainfragments.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/addToSaleFragment"
        android:name="com.jamshidbek.marketplaceinc.mainfragments.AddToSaleFragment"
        android:label="fragment_add_to_sale"
        tools:layout="@layout/fragment_add_to_sale">
    </fragment>

    <fragment
