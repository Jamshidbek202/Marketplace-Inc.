    app:startDestination="@id/onSaleFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.jamshidbek.marketplaceinc.mainfragments.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search">
    </fragment>

    <fragment
        android:id="@+id/onSaleFragment"
        android:name="com.jamshidbek.marketplaceinc.mainfragments.OnSaleFragment"
        android:label="fragment_on_sale"
